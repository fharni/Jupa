    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>courses</from-outcome>
            <to-view-id>/courses/courses.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>federation</from-outcome>
            <to-view-id>/federation/federation.xhtml</to-view-id>
